{
  "name": "Eliminator Slajdów",
  "manifest_version": 2,
  "version": "4.0.3",
  "homepage_url": "https://eliminator-slajdow.herokuapp.com/",
  "description": "Dodatek eliminuje slajdy w galeriach zdjęć na większości polskich portali",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "options_ui": {
    "page": "html/options.html"
  },
  "icons": {
    "48": "images/es_logo.svg"
  },
  "browser_action": {
    "default_icon": "images/enableIcon.png",
    "default_title": "Eliminator Slajdów",
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    "images/es_logo.svg"
  ],
  "background": {
    "scripts": [
      "js/jquery-3.1.1.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./js/contentscript.js"
      ]
    }
  ]
}