{
  "name": "Eliminator Slajdów",
  "manifest_version": 2,
  "version": "4.0.0",
  "homepage_url": "https://github.com/maestr0/eliminator-slajdow",
  "description": "Dodatek eliminuje slajdy w galeriach zdjęć na większości polskich portali",
  "permissions": [
    "activeTab"
  ],
  "options_ui": {
    "page": "html/options.html"
  },
  "icons": {
    "48": "images/es_logo.svg"
  },
  "browser_action": {
    "default_icon": "images/icon_48.png",
    "default_title": "Eliminator Slajdów",
    "default_popup": "html/options.html"
  },
  "web_accessible_resources": [
    "images/DonateBitcoin.png",
    "images/es_logo.svg"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./js/contentscript.js"
      ]
    }
  ]
}