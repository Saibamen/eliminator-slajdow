{
    "name": "Wszystkie zdjęcia na raz. GALERIA gazeta.pl",
    "manifest_version": 2,
    "version": "1.0.10",
    "description": "Ulepszony interfejs galerii zdjęc w serwisach AGORY. Wszystkie zdjęcia wyświetlają się na jednej stronie.",
    "page_action": {
        "default_title": "Galeria Gazeta.pl"
    },
    "permissions": [
        "tabs"
    ],
    "incognito": "spanning",
	"content_security_policy": "script-src 'self'; object-src 'self'",
    "icons": {
        "16": "icon_16.jpg",
        "48": "icon_48.jpg",
        "128": "icon_128.jpg"
    },
    "content_scripts": [
        {
            "matches": [ "http://*/*"
            ],
            "js": [
                "jquery-1.7.1.min.js",
                "contentscript.js"
            ],
            "run_at": "document_idle",
            "all_frames": false
        }
    ]
}
