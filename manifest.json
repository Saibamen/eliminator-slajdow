{
    "name": "Eliminator Slajdów (Lepsza Gazeta.pl)",
    "manifest_version": 2,
    "version": "1.2.5",
    "description": "Ulepszony interfejs galerii zdjęc w serwisach AGORY. Wszystkie zdjęcia wyświetlają się na jednej stronie.",
    "page_action": {
        "default_icon": "icon_16.jpg",
        "default_title": "Eliminator Slajdów",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs"],
    "incognito": "spanning",
    "options_page": "popup.html",
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "icons": {
        "16": "icon_16.jpg",
        "48": "icon_48.jpg",
        "128": "icon_128.jpg"
    },
    "background": {
        "page": "background.html"
    },
    "content_scripts": [{
        "matches": ["http://*/*"],
        "js": ["jquery-1.8.3.min.js", "contentscript.js"],
        "run_at": "document_idle",
        "all_frames": false
    }],
    "web_accessible_resources": ["ajax-loader.gif"]
}
